<!--/pages/managemember/managemember.wxml-->
<view class="">
  <view class="userinfo">
    <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    <view class="bind-account">{{username}}</view>
  </view>
  <navigator url="/pages/myOrder/myOrder" class="list">管理车辆</navigator>
  <navigator url="/pages/myOrder/myOrder" class="list">管理司机</navigator>
  <button>退出账号</button>
  <view class="footer" style="top:{{footHeight}}px;">
    <view class="tab" hover-class="tab-hover" hover-stay-time="100" bindtap="bottomTab1">订单</view>
    <view class="tab" hover-class="tab-hover" hover-stay-time="100" bindtap="bottomTab2">人员</view>
  </view>
</view>